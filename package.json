{
  "name": "@coool/route-node",
  "version": "2.0.0",
  "scripts": {
    "build": "npm run clean && npm run build:compile && npm run build:copyFiles",
    "clean": "rm -rf dist",
    "build:copyFiles": "cp ./{LICENSE,README.md,package.json,package-lock.json} ./dist/",
    "build:compile": "npx tsc",
    "watch": "npx tsc -w",
    "release": "cd ./dist && npm publish --access=public && cd .."
  },
  "dependencies": {
  },
  "description": "Utilities for easy route node definitions.",
  "devDependencies": {
    "prettier": "^3.4.1",
    "prettier-plugin-multiline-arrays": "^3.0.6",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hacklone/coool-route-node.git"
  },
  "keywords": [
    "typescript",
    "route",
    "node",
    "definition",
    "API"
  ],
  "author": "Hacklone <toakak@gmail.com>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/Hacklone/coool-route-node/raw/main/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/Hacklone/coool-route-node/issues"
  },
  "homepage": "https://github.com/Hacklone/coool-route-node"
}
